             function compararnumeros (x,y) {

if (x > y) { 
   console.log (`O número ${x} é maior que ${y}`)
   return;
}
if (y > x){
    console.log (`O número ${y} é maior que ${x}`)
    return;
}
    console.log (`O número ${y} é igual a ${x}`)
}

   
compararnumeros (5,3)





        </script>


